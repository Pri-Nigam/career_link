<h1>Our happy Clients</h1>
<ul>
<% @clients.each do |user|%>
  <li>
    <%= link_to user.name, client_path(user.id) %>
  </li>
<% end %>
</ul>
<br>
<%= link_to "New Job", new_job_path %>
<br>
<% if user_signed_in? %>
  <%= link_to "Sign out", destroy_user_session_path %>
<% else %>
  <%= link_to "Sign In", new_user_session_path %>
<%end%>
